[sdl]
fullscreen=false
fulldouble=false
fullresolution=original
windowresolution=1024x768
output=overlay
autolock=true
sensitivity=100
waitonerror=true
priority=higher,pause
mapperfile=/home/ootput/.dosbox/mapper.txt
usescancodes=true

[dosbox]
language=
machine=vga
captures=/home/ootput/.dosbox/captures
memsize=32

[render]
frameskip=0
aspect=true
scaler=normal3x

[cpu]
core=dynamic
cycles=20000

[mixer]
nosound=false
rate=22050
blocksize=2048
prebuffer=30

[midi]
mpu401=intelligent
device=alsa
config=128:0

[sblaster]
sbtype=sb16
sbbase=220
irq=7
dma=1
hdma=5
mixer=true
oplmode=auto
oplrate=22050

[gus]
gus=false
gusrate=22050
gusbase=220
irq1=7
irq2=7
dma1=1
dma2=5
ultradir=/etc/timidity/freepats.cfg

[speaker]
pcspeaker=false
pcrate=22050
tandy=auto
tandyrate=22050
disney=true

[joystick]
joysticktype=none
timed=true
autofire=false
swap34=false
buttonwrap=true

[serial]
serial1=dummy
serial2=dummy
serial3=disabled
serial4=disabled

[dos]
xms=true
ems=true
umb=true
keyboardlayout=US

[ipx]
ipx=false

[autoexec]
mount c /media/seagate-1.5T/games/roms/dos
C:
